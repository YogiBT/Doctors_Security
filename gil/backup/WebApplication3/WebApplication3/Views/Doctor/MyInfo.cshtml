@model WebApplication3.Models.Doctor
@{
    ViewBag.Title = "MyInfo";
    Layout = "~/Views/Shared/_LayoutDoctor.cshtml";
}
@*
    show current doctor information.    

*@
<h2>MyInfo</h2>

<body>
    <head>
        Welcome Doctor @Session["DoctorLoggedIn"]
    </head>
    <br />
    DoctorInfo:
    <br />
    <table class="table">
        <tr>
            <th> First Name </th>
            <th> Last Name </th>
            <th> Age </th>
            <th> Phone </th>
            <th> Email </th>

        </tr>
        @foreach (var item in ViewBag.TempDoc)
        {
            <tr>
                <td>@item.FirstName  </td>
                <td>@item.LastName </td>
                <td>@item.Age </td>
                <td>@item.Phone </td>
                <td>@item.Email </td>
                <td>@Html.ActionLink("Edit", "Edit")</td>
            </tr>

        }




    </table>
    <br />
